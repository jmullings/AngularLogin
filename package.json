{
  "name": "angular-login",
  "version": "1.0.1",
  "scripts": {
    "clean": "rimraf doc dist coverage",
    "start:cli": "webpack-dev-server --config webpack.dev.js --inline --progress --profile --port 8080",
    "build": "npm run clean && webpack --progress --profile --bail --config webpack.release.js",
    "docs": "typedoc --options typedoc.json src/app/app.component.ts",
    "test": "karma start && remap-istanbul -i coverage/json/coverage-final.json -o coverage/html -t html",
    "lint": "tslint --force \"src/**/*.ts\"",
    "ng": "ng",
    "start": "node server.js",
    "e2e": "ng e2e",
    "postinstall": "ng build --aot --prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "body-parser": "^1.18.3",
    "core-js": "^2.5.4",
    "css-loader": "^1.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.3.2",
    "to-string-loader": "^1.1.5",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "~6.1.1",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "html-webpack-exclude-assets-plugin": "0.0.7",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "uglifyjs-webpack-plugin": "1.2.7",
    "url-loader": "1.0.1",
    "webpack": "4.9.2",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "3.1.4",
    "webpack-merge": "^4.1.2"
  }
}
